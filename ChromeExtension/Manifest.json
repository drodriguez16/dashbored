{
    "name": "FileByrd",
    "version": "1.0.0",
    "description": "Launch FileByrd.com",
    "manifest_version": 2,
    "icons": {
        "16": "images/16.png",
        "128": "images/128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "loadfilebyrd.js"
            ]
        }
    ]
}